<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Acquisition_Cost__c</fullName>
    <description>Unloaded marketing CAC, does not include overhead.</description>
    <externalId>false</externalId>
    <formula>IF( NumberOfWonOpportunities = 0, NULL , 
(Actual_Cost_in_Campaign__c /  NumberOfWonOpportunities))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Unloaded marketing CAC, does not include overhead.</inlineHelpText>
    <label>Acquisition Cost</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <type>Currency</type>
</CustomField>
